@model IEnumerable<WebOlimpiada.DatosRepositorio.Modelos.Equipamento>

@{
    ViewBag.Title = "Equipos";
}

<h2>Equipos y Materiales</h2>

<p>
    @Html.ActionLink("Crear", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EquipamentoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EquipamentoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EquipamentoId })
        </td>
    </tr>
}

</table>
