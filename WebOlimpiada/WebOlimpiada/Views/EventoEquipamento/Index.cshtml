@model IEnumerable<WebOlimpiada.DatosRepositorio.Modelos.EventoEquipamento_View>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear", "Create", "EventoEquipos", new { eventoId = ViewData["EventoId"] }, new { })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cantidad)
        </td>
        <td>
            @Html.ActionLink("Actualizar", "Edit", "EventoEquipos", new { eventoId = item.EventoId, equipamentoId= item.EquipamentoId  }, new { }) |
            @Html.ActionLink("Delete", "Delete", "EventoEquipos", new  { eventoId = item.EventoId, equipamentoId = item.EquipamentoId }, new { })
        </td>
    </tr>
}

</table>
